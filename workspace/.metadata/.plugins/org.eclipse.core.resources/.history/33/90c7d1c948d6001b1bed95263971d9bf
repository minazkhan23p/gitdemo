

import java.util.ArrayList;

public class cwd {

public static void main(String[] args) { // TODO Auto-generated method stub

String a ="hello world"; // Print unique number from the list- Amazon  //print the string in reverse 
ArrayList<String>ab =new ArrayList<String>(); 
for(int i=0;i<a.length();i++) { 
	int k=0; 
	if(!ab.contains(a.charAt(i))) { 
		
		ab.add(Character.toString(a.charAt(i)));
		k++; 
		for(int j=i+1;j<a.length();j++) { 
			if(a.charAt(i)==a.charAt(j)) { k++; } } // 
		System.out.println(Character.toString(a.charAt(i))); //
		System.out.println(k); 
		if(k==1){ System.out.println(Character.toString(a.charAt(i))); 
		break;} } 
	}
		}
	
}

